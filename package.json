{
  "name": "tex-lab",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "setup": "node scripts/setup.js",
    "clean": "rm -rf node_modules package-lock.json .arch-lock && npm install",
    "clean:run": "rm -rf node_modules package-lock.json && npm install && npm run dev",
    "clean:full": "npm run clean && npm run setup",
    "cache:clear": "rm -rf .astro dist node_modules/.cache",
    "dev": "astro dev",
    "build": "npm run cache:clear && astro build",
    "preview": "astro preview"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/tailwind": "^5.1.3",
    "@tailwindcss/typography": "^0.5.15",
    "astro": "^5.17.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3"
  }
}
